#include <stdio.h>
#include <string.h>
int main(){
	char first = '`';
	char second1 = '\'';
	char second2 = '\0\'';

	char goal = '\'' ;
	char arr[1000];
	int flag = 1;
	int len = strlen(arr);

	while(scanf("%s", arr) != EOF){
		int len = strlen(arr);
		for(int i =0; i<len; i++){
			if ((arr[i] = goal) && (arr[i+1] = goal)){
				if (flag ==1){
					arr[i] = first;
					arr[i+1] = first;
					flag = 0;
				}else{
					arr[i] = second1;
					arr[i+1]= second2;
					flag = 1;
				}
			}
		}
		printf("%s\n", arr);
	}
}
